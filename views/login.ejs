<%- include('partials/header') %>
<form action="/login" class="form-signin" method="POST">
  <h1>Login</h1>
  <hr />
  <label class="sized-box" for="username">Username</label>
  <input type="username" class="form-control" name="username">
  <label class="sized-box" for="password">Password</label>
  <input type="password" class="form-control bottom-form" name="password">
  <button type="submit" class="btn btn-lg btn-dark btn-block">Login</button>
</form>

<%- include('partials/footer') %>
